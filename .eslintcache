s/vending-machine/src/containers/menu/MenuWindow.tsx",[],"/home/kristina/MyProjects/vending-machine/src/containers/vending-machine/VendingMachineContainer.tsx",[],"/home/kristina/MyProjects/vending-machine/src/containers/vending-machine/VendingMachine.tsx",[],["192","193"],"/home/kristina/MyProjects/vending-machine/src/containers/vending-machine/products/ProductList.tsx",[],"/home/kristina/MyProjects/vending-machine/src/containers/menu/MenuContainer.tsx",["194"],"import React, { useEffect, useState } from \"react\";\nimport MenuButton from \"./MenuButton\";\nimport MenuWindow from \"./MenuWindow\";\nimport { BrowserRouter as Router, Switch, Route, Link } from \"react-router-dom\";\nimport BasketContainer from \"../basket/BasketContainer\";\nimport VendingMachineContainer from \"../vending-machine/VendingMachineContainer\";\nimport WalletContainer from \"../wallet/WalletContainer\";\nimport CoinSlotContainer from \"../coin-slot/CoinSlotContainer\";\nimport MenuLink from \"./MenuLink\";\nimport Subtitle from \"../../components/main/Subtitle\";\nimport Title from \"../../components/main/Title\";\n\nconst MenuContainer = () => {\n  const [openFull, setOpenFull] = useState(false);\n\n  useEffect(() => {\n    let timeout: NodeJS.Timeout;\n    timeout = setTimeout(() => {}, 5000);\n    return () => {\n      clearTimeout(timeout);\n    };\n  }, [openFull]);\n\n  return (\n    <Router>\n      <MenuButton\n        onClick={() => setOpenFull(!openFull)}\n        id={openFull ? \"open\" : \"close\"}\n      >\n        {!openFull ? (\n          <i className=\"fas fa-bars fa-lg\" />\n        ) : (\n          <i className=\"fas fa-times fa-lg\" />\n        )}\n      </MenuButton>\n      <Switch>\n        <Route path=\"/basket\">\n          <BasketContainer />\n        </Route>\n        <Route path=\"/wallet\">\n          <WalletContainer />\n        </Route>\n        <Route exact path=\"/\">\n          <VendingMachineContainer />\n        </Route>\n        <Route exact path=\"/coin-slot\">\n          <CoinSlotContainer />\n        </Route>\n      </Switch>\n      {openFull ? (\n        <MenuWindow>\n          <Title>Vending Machine</Title>\n          <MenuLink onClick={() => setOpenFull(false)}>\n            <Link to=\"/\">Machine</Link>\n          </MenuLink>\n          <MenuLink onClick={() => setOpenFull(false)}>\n            <Link to=\"/wallet\">Wallet</Link>\n          </MenuLink>\n          <MenuLink onClick={() => setOpenFull(false)}>\n            <Link to=\"/basket\">Basket</Link>\n          </MenuLink>\n          <MenuLink onClick={() => setOpenFull(false)}>\n            <Link to=\"/coin-slot\">Coin Slot</Link>\n          </MenuLink>\n        </MenuWindow>\n      ) : null}\n    </Router>\n  );\n};\n\nexport default MenuContainer;\n","/home/kristina/MyProjects/vending-machine/src/redux-store/basket/basket.slice.ts",["195","196"],"import { createSlice, PayloadAction } from \"@reduxjs/toolkit\";\nimport { buyProduct, Product } from \"../vending-machine/vendingMachine.slice\";\nimport apple from \"../../images/apple.jpg\";\nimport cola from \"../../images/cola.png\";\n\nexport interface Basket {\n  boughtProducts: { product: Product; quantity: number }[];\n}\n\nconst initialState: Basket = {\n  boughtProducts: [],\n};\n\nconst { reducer: basketReducer, actions } = createSlice({\n  name: \"basket\",\n  initialState,\n  reducers: {\n    addProduct: (state, { payload }: PayloadAction<Product>) => {\n      console.log(\"payload\", payload);\n      const index = state.boughtProducts.findIndex(\n        ({ product: { id } }) => id === payload.id\n      );\n      console.log(\"index\", index);\n      if (index >= 0) {\n        state.boughtProducts[index].quantity += 1;\n        console.log(\"boughtProducts\", state.boughtProducts);\n      } else {\n        state.boughtProducts.push({ product: payload, quantity: 1 });\n        console.log(\"boughtProducts\", state.boughtProducts);\n      }\n    },\n  },\n  extraReducers: (builder) => {\n    builder.addCase(buyProduct, (state, { payload }) => {\n      // console.log(\"payload\", payload);\n      const index = state.boughtProducts.findIndex(\n        ({ product: { id } }) => id === payload.id\n      );\n      // console.log(\"index\", index);\n      if (index >= 0) {\n        state.boughtProducts[index].quantity += 1;\n        // console.log(\"boughtProducts\", state.boughtProducts);\n      } else {\n        state.boughtProducts.push({ product: payload, quantity: 1 });\n        // console.log(\"boughtProducts\", state.boughtProducts);\n      }\n    });\n  },\n});\n\nexport const { addProduct } = actions;\nexport default basketReducer;\n","/home/kristina/MyProjects/vending-machine/src/containers/basket/BasketContainer.tsx",[],"/home/kristina/MyProjects/vending-machine/src/redux-store/basket/basket.selector.ts",[],"/home/kristina/MyProjects/vending-machine/src/containers/basket/BoughtProductContainer.tsx",[],"/home/kristina/MyProjects/vending-machine/src/containers/basket/BoughtProduct.tsx",[],"/home/kristina/MyProjects/vending-machine/src/components/main/MainWindow.tsx",[],"/home/kristina/MyProjects/vending-machine/src/containers/vending-machine/control-panel/ControlPanelContainer.tsx",["197"],"import React, { useEffect, useState } from \"react\";\nimport { CURRENCY, DELIMITER, CODE_LENGTH } from \"../../../consts\";\nimport { buyProduct } from \"../../../redux-store/vending-machine/vendingMachine.slice\";\nimport { slotListSelector } from \"../../../redux-store/vending-machine/vendingMachine.selector\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport ControlPanelComponent from \"./ControlPanelComponent\";\nimport { coinSlotSelector } from \"../../../redux-store/coin-slot/coinSlot.selector\";\n\nconst ControlPanelConteiner = () => {\n  const slots = useSelector(slotListSelector);\n  const { total: money } = useSelector(coinSlotSelector);\n  const dispatch = useDispatch();\n  const [code, setCode] = useState(\"\");\n\n  useEffect(() => {\n    let timeout: NodeJS.Timeout;\n    if (code.length === CODE_LENGTH) {\n      timeout = setTimeout(() => {\n        submitCode();\n      }, 700);\n    }\n    return () => {\n      clearTimeout(timeout);\n    };\n  }, [code]);\n\n  const submitCode = () => {\n    const slot = slots.find((slot) => slot.code === code);\n    if (slot && slot.quantity > 0 && money - slot.product.price >= 0) {\n      dispatch(buyProduct(slot.product));\n    }\n    setCode(\"\");\n  };\n\n  const appendDigitToCode = (digit: number) => {\n    if (code.length < CODE_LENGTH) {\n      const newCode = code.concat(digit.toString());\n      setCode(newCode);\n    }\n  };\n\n  return (\n    <ControlPanelComponent\n      display={code ? code : `${(money / DELIMITER).toString()}${CURRENCY}`}\n      appendDigit={appendDigitToCode}\n      resetCode={() => setCode(\"\")}\n    />\n  );\n};\n\nexport default ControlPanelConteiner;\n","/home/kristina/MyProjects/vending-machine/src/containers/vending-machine/control-panel/ControlPanelComponent.tsx",[],"/home/kristina/MyProjects/vending-machine/src/containers/vending-machine/products/ProductListContainer.tsx",[],"/home/kristina/MyProjects/vending-machine/src/containers/vending-machine/products/product/ProductComponent.tsx",[],"/home/kristina/MyProjects/vending-machine/src/containers/wallet/WalletContainer.tsx",[],"/home/kristina/MyProjects/vending-machine/src/containers/coin-slot/CoinSlotContainer.tsx",[],"/home/kristina/MyProjects/vending-machine/src/containers/coin-slot/CoinSlot.tsx",["198"],"import React from \"react\";\nimport MainWindow from \"../../components/main/MainWindow\";\nimport Subtitle from \"../../components/main/Subtitle\";\nimport Title from \"../../components/main/Title\";\nimport { CURRENCY, DELIMITER, MONEY_AMOUNTS } from \"../../consts\";\nimport KeypadButton from \"../vending-machine/control-panel/KeypadButton\";\nimport TextArea from \"../vending-machine/control-panel/TextArea\";\nimport Coin from \"./Coin\";\nimport CoinList from \"